  "repository": "git@github.com:onlyreynaldo/special-api-ts.git",
  "author": "Layme, Reynaldo <layme.reynaldo@icloud.com>",
  "license": "MIT",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.9",
    "@types/faker": "^5.1.6",
    "@types/jest": "^26.0.19",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.6.3",
    "@types/node": "^14.14.19",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validator": "^13.1.2",
    "@typescript-eslint/eslint-plugin": "3",
    "bson-objectid": "^1.3.1",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.1.0",
    "eslint": "7",
    "eslint-config-standard-with-typescript": "19.0.0",
    "eslint-plugin-import": "2",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "4",
    "eslint-plugin-standard": "4",
    "faker": "^5.3.1",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "mockdate": "^3.0.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "postbuild": "copyfiles -u 1 public/**/* dist/static",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "up": "yarn build && docker-compose up -d",
    "down": "docker-compose down",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand --no-cache",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "yarn test --watch -c jest-unit-config.ts",
    "test:integration": "yarn test --watch -c jest-integration-config.ts",
    "test:staged": "yarn test --findRelatedTests",
    "test:ci": "yarn test --coverage",
    "test:coveralls": "yarn test:ci && coveralls < coverage/lcov.info"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongodb": "^3.6.3",
    "nodemon": "^2.0.7",
    "swagger-ui-express": "^4.1.6",
    "validator": "^13.5.2"
  },
  "engines": {
    "node": "12.x"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "config": {
    "mongodbMemoryServer": {
      "version": "4.4.3"
    }
  }
}
